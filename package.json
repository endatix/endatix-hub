  "engines": {
    "pnpm": ">=9.0.0 <=10.30.0",
    "node": ">=20.9.0 <25.1.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2",
      "glob": "10.5.0",
      "vite": "^7.1.11",
      "js-yaml": "^4.1.1",
      "preact": "10.27.3"
    },
