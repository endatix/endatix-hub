{
  "name": "endatix-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm discover-questions && cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 next dev --turbopack",
    "dev-https": "pnpm dev --experimental-https",
    "dev:inspect": "pnpm discover-questions && cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "pnpm discover-questions && next build --turbopack",
    "build:standalone": "pnpm build && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "run:standalone": "pnpm build:standalone && NODE_TLS_REJECT_UNAUTHORIZED=0 node .next/standalone/server.js --trace-warnings",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test --project=smoke",
    "test:e2e:debug": "playwright test --debug",
    "discover-questions": "node scripts/discover-questions.mjs",
    "pnpm:comments": "node scripts/echo-pnpm-comments.mjs"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry-exporter": "1.0.0-beta.32",
